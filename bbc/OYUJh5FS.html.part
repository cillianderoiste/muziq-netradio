				//Audio popup fix
				glow.dom.get('a.pclLink').filter(function(e) {
					if (this.className.match('popmp3')) {
		
						//Move mp3 url out of href
						var mp3Link = glow.dom.get('a.pclLink').attr('href');
						glow.dom.get('a.pclLink').attr('href','#');						
